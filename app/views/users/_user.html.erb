<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Age:</strong>
    <%= user.age %>
  </p>

  <p>
    <strong>Height:</strong>
    <%= user.height %>
  </p>

  <p>
    <strong>Weight:</strong>
    <%= user.weight %>
  </p>

  <p>
    <strong>Gender:</strong>
    <%= user.gender %>
  </p>

</div>
